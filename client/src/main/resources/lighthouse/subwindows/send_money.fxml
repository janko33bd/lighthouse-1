<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox alignment="CENTER" prefHeight="200.0" prefWidth="600.0" spacing="15.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lighthouse.subwindows.EmptyWalletController">
    <children>
        <Label fx:id="titleLabel" maxWidth="1.7976931348623157E308" text="%Send to address:" />
        <TextField fx:id="address" prefWidth="354.0" promptText="BARqrxd8Enb3LgHK6VXrqKmYwnnnxpfdaH">
            <VBox.margin>
                <Insets />
            </VBox.margin>
        </TextField>
        <HBox alignment="CENTER_RIGHT" fillHeight="true" prefHeight="30.0" prefWidth="600.0" spacing="15.0" VBox.vgrow="NEVER">
            <children>
                <Button fx:id="cancelBtn" cancelButton="true" mnemonicParsing="false" onAction="#cancel" minWidth="80.0" text="%Cancel" />
                <Button fx:id="sendBtn" defaultButton="true" mnemonicParsing="false" onAction="#send" minWidth="80.0" text="%Send" />
            </children>
        </HBox>
    </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
